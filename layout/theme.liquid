    <script>
      document.addEventListener('DOMContentLoaded', () => {
        setModalLpMoreDetails()
        setModalLpMoreDetailsDE()

        function setModalLpMoreDetails() {
          const modal = new tingle.modal({
            footer: false,
            stickyFooter: false,
            closeMethods: ['overlay', 'button', 'escape'],
            cssClass: ['lp-more-details'],
          });

          // set content
          modal.setContent(`
            <p><b>Asia</b><br>
            China, Hong Kong, India, Indonesia, Japan, Malaysia, Philippines, Singapore, South Korea, Sri Lanka, Taiwan, Thailand, Türkiye, United Arab Emirates(Abu Dhabi, Dubai)<br>
            <b>Europe</b><br>
            Belgium, Denmark, Finland, France, Germany, Ireland, Italy, Latvia, Netherlands, Norway, Poland, Slovenia, Spain, Sweden, Switzerland, United Kingdom<br>
            <b>Oceania</b><br>
            Australia, New Zealand<br>
            <b>North America</b><br>
            Canada, Mexico, United States<br>
            <b>South America</b><br>
            Argentina, Brazil, Peru<br>
            <b>Africa</b><p>
          `);

          const openEl = document.querySelector('a[href="#open-lp-more-details-modal"]')
          if (openEl) {
            openEl.addEventListener('click', (ev) => {
              ev.preventDefault()
              modal.open();
            })
          }
        }

        function setModalLpMoreDetailsDE() {
          const modal = new tingle.modal({
            footer: false,
            stickyFooter: false,
            closeMethods: ['overlay', 'button', 'escape'],
            cssClass: ['lp-more-details'],
          });

          // set content
          modal.setContent(`
            <p><b>Asien</b><br>
            China, Hongkong, Indien, Indonesien, Japan, Malaysia, Philippinen, Singapur, Südkorea, Sri Lanka, Taiwan, Thailand, Türkiye, Vereinigte Arabische Emirate (Abu Dhabi, Dubai)<br>
            <b>Europa</b><br>
            Belgien, Dänemark, Finnland, Frankreich, Deutschland, Irland, Italien, Lettland, Niederlande, Norwegen, Polen, Slowenien, Spanien, Schweden, Schweiz, Vereinigtes Königreich<br>
            <b>Ozeanien</b><br>
            Australien, Neuseeland<br>
            <b>Nordamerika</b><br>
            Kanada, Mexiko, Vereinigte Staaten<br>
            <b>Südamerika</b><br>
            Argentinien, Brasilien, Peru<br>
            <b>Afrika</b><br>
            Ghana<p>
          `);

          const openEl = document.querySelector('a[href="#open-lp-more-details-modal-de"]')
          if (openEl) {
            openEl.addEventListener('click', (ev) => {
              ev.preventDefault()
              modal.open();
            })
          }
        }
      })
    </script>

    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/toastify-js/src/toastify.min.css">
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>
    <script>
      document.addEventListener('DOMContentLoaded', () => {
        const announcementBar = document.querySelector('#shopify-section-announcement-bar .is-coupon')
        const url = announcementBar ? announcementBar.getAttribute('href') : null
        if (announcementBar) {
          announcementBar.addEventListener('click', (ev) => {
            ev.preventDefault()
            Toastify({
              text: "Successfully got coupon code!",
              duration: 3000,
              gravity: "top", // `top` or `bottom`
              position: "center", // `left`, `center` or `right`
              stopOnFocus: true, // Prevents dismissing of toast on hover
              offset: {
                y: 50
              },
              onClick: function(){} // Callback after click
            }).showToast();
            setTimeout(() => {
              if (url) {
                location.href = url
              }
            }, 2000)
          })
        }
      })
    </script>


