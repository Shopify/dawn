      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "{% comment %}\n  USA Price Disclaimer - Single File Liquid Snippet\n  Add this snippet to your theme and include it with: {% render 'usa-disclaimer-snippet' %}\n{% endcomment %}\n\n<style>\n  /* Disclaimer icon styles */\n  s.shopseal-disclaimer::after,\n  del.shopseal-disclaimer::after,\n  .price-item--regular.shopseal-disclaimer::after,\n  .price__compare-at.shopseal-disclaimer::after,\n  .was-price.shopseal-disclaimer::after,\n  .compare-at-price.shopseal-disclaimer::after {\n    content: 'ⓘ';\n    display: inline-block;\n    margin-left: 4px;\n    font-size: 0.75em;\n    vertical-align: super;\n    color: #666;\n    cursor: help;\n    font-weight: normal;\n    font-style: normal;\n    text-decoration: none;\n  }\n\n  /* Hover effect */\n  .shopseal-disclaimer:hover::after {\n    opacity: 1;\n    transform: scale(1.1);\n  }\n\n  /* Make elements clickable */\n  .shopseal-disclaimer {\n    cursor: pointer;\n    text-decoration-style: dotted;\n    text-decoration-thickness: 1px;\n    position: relative;\n  }\n\n  /* Focus styles for accessibility */\n  .shopseal-disclaimer:focus-visible {\n    outline: 2px solid #0066cc;\n    outline-offset: 2px;\n    border-radius: 2px;\n  }\n\n  /* Popup styles */\n  .shopseal-popup {\n    position: fixed;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    background: white;\n    color: #333;\n    padding: 20px;\n    border-radius: 8px;\n    max-width: 420px;\n    width: calc(100% - 48px);\n    box-shadow: 0 8px 32px rgba(0,0,0,0.35);\n    max-height: 80vh;\n    overflow: auto;\n    z-index: 999999;\n    display: none;\n  }\n\n  .shopseal-popup.active {\n    display: block;\n  }\n\n  .shopseal-popup-backdrop {\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    background: rgba(0,0,0,0.35);\n    z-index: 999998;\n    display: none;\n  }\n\n  .shopseal-popup-backdrop.active {\n    display: block;\n  }\n\n  .shopseal-popup-close {\n    position: absolute;\n    top: 10px;\n    right: 10px;\n    background: transparent;\n    border: none;\n    font-size: 20px;\n    cursor: pointer;\n    color: #666;\n    padding: 0;\n    width: 30px;\n    height: 30px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    border-radius: 50%;\n    transition: background 0.2s;\n  }\n\n  .shopseal-popup-close:hover {\n    background: #f0f0f0;\n  }\n\n  .shopseal-popup-content {\n    margin-top: 10px;\n    line-height: 1.6;\n  }\n\n  /* Mobile responsive */\n  @media (max-width: 480px) {\n    .shopseal-popup {\n      width: calc(100% - 40px);\n      max-width: none;\n    }\n  }\n\n  /* Print styles */\n  @media print {\n    .shopseal-popup,\n    .shopseal-popup-backdrop,\n    .shopseal-disclaimer::after {\n      display: none !important;\n    }\n  }\n</style>\n\n<div class=\"shopseal-popup-backdrop\" id=\"shopseal-backdrop\"></div>\n<div class=\"shopseal-popup\" id=\"shopseal-popup\" role=\"dialog\" aria-modal=\"true\" aria-label=\"Price disclaimer\">\n  <button class=\"shopseal-popup-close\" id=\"shopseal-close\" aria-label=\"Close disclaimer\">✕</button>\n  <div class=\"shopseal-popup-content\">\n    <strong>Price Drop in Effect</strong><br><br>\n    The New Lower Price reflects a recent markdown from our regular retail price.<br><br>\n    You may also see a Previous Price shown with a strikethrough on search results, product pages, or at checkout.<br><br>\n    The Previous Price is based on the median price offered for this item on our website over the last 90 days (before any discount codes at checkout).\n  </div>\n</div>\n\n<script>\n  (function() {\n    'use strict';\n\n    //  disclaimer text (kept for reference, but using HTML version in popup)\n    const disclaimerText = 'Price Drop in Effect. The New Lower Price reflects a recent markdown from our regular retail price. You may also see a Previous Price shown with a strikethrough on search results, product pages, or at checkout. The Previous Price is based on the median price offered for this item on our website over the last 90 days (before any discount codes at checkout).';\n\n    // Selectors for compare-at prices\n    const priceSelectors = [\n      's', \n      'del', \n      '.price-item--sale + .price-item--regular',\n      '.price-item--regular:has(+ .price-item--sale)',\n      '.price__compare-at',\n      '.price--compare',\n      '.was-price',\n      '.compare-at-price',\n      '.product-price-old',\n      '[data-compare-at-price]',\n      '[data-compare-price]'\n    ];\n\n    let popup = null;\n    let backdrop = null;\n    let activeTarget = null;\n\n    function init() {\n      popup = document.getElementById('shopseal-popup');\n      backdrop = document.getElementById('shopseal-backdrop');\n      \n      if (!popup || !backdrop) return;\n\n      // Close button handler\n      const closeBtn = document.getElementById('shopseal-close');\n      if (closeBtn) {\n        closeBtn.addEventListener('click', hidePopup);\n      }\n\n      // Backdrop click to close\n      backdrop.addEventListener('click', hidePopup);\n\n      // Escape key to close\n      document.addEventListener('keydown', function(e) {\n        if (e.key === 'Escape' && popup.classList.contains('active')) {\n          hidePopup();\n        }\n      });\n\n      // Initial scan and setup\n      scanAndProcess();\n\n      // Watch for new elements\n      const observer = new MutationObserver(function() {\n        setTimeout(scanAndProcess, 300);\n      });\n\n      observer.observe(document.body, {\n        childList: true,\n        subtree: true,\n        attributes: true,\n        attributeFilter: ['class', 'style']\n      });\n    }\n\n    function scanAndProcess() {\n      const selector = priceSelectors.join(',');\n      const elements = document.querySelectorAll(selector);\n      \n      elements.forEach(function(el) {\n        // Skip if already processed\n        if (el.classList.contains('shopseal-disclaimer')) return;\n        \n        // Check if it's a valid compare-at price\n        if (!isValidCompareAt(el)) return;\n        \n        // Add disclaimer class and make interactive\n        el.classList.add('shopseal-disclaimer');\n        el.setAttribute('role', 'button');\n        el.setAttribute('aria-haspopup', 'dialog');\n        el.setAttribute('aria-expanded', 'false');\n        el.tabIndex = 0;\n        \n        // Click handler\n        el.addEventListener('click', function(e) {\n          e.preventDefault();\n          e.stopPropagation();\n          showPopup(el);\n        });\n        \n        // Keyboard handler\n        el.addEventListener('keydown', function(e) {\n          if (e.key === 'Enter' || e.key === ' ') {\n            e.preventDefault();\n            showPopup(el);\n          }\n        });\n      });\n    }\n\n    function isValidCompareAt(el) {\n      const text = el.textContent.trim();\n      \n      // Check for empty or placeholder text\n      if (!text || text === '—' || text === '-' || text === '--') return false;\n      \n      // Check for price-like content\n      if (!/[\\d$€£¥₹¢]/.test(text) || !/\\d/.test(text)) return false;\n      \n      // Check visibility\n      const isVisible = !!(el.offsetWidth || el.offsetHeight || el.getClientRects().length);\n      if (!isVisible) return false;\n      \n      // Check for strikethrough elements or classes\n      const tagName = el.tagName.toLowerCase();\n      if (tagName === 's' || tagName === 'del') return true;\n      \n      // Check computed styles\n      const styles = window.getComputedStyle(el);\n      if (styles.textDecoration.includes('line-through')) return true;\n      \n      // Check for nested strikethrough\n      if (el.querySelector('s, del')) return true;\n      \n      // Check class names\n      const classList = el.className || '';\n      if (typeof classList === 'string' && classList.match(/compare|was|old/i)) return true;\n      \n      // Check data attributes\n      return el.hasAttribute('data-compare-at-price') || el.hasAttribute('data-compare-price');\n    }\n\n    function showPopup(target) {\n      if (!popup || !backdrop) return;\n      \n      activeTarget = target;\n      target.setAttribute('aria-expanded', 'true');\n      \n      popup.classList.add('active');\n      backdrop.classList.add('active');\n      \n      // Focus management\n      const closeBtn = document.getElementById('shopseal-close');\n      if (closeBtn) {\n        setTimeout(function() {\n          closeBtn.focus();\n        }, 100);\n      }\n    }\n\n    function hidePopup() {\n      if (!popup || !backdrop) return;\n      \n      popup.classList.remove('active');\n      backdrop.classList.remove('active');\n      \n      if (activeTarget) {\n        activeTarget.setAttribute('aria-expanded', 'false');\n        activeTarget.focus();\n        activeTarget = null;\n      }\n    }\n\n    // Initialize when DOM is ready\n    if (document.readyState === 'loading') {\n      document.addEventListener('DOMContentLoaded', init);\n    } else {\n      init();\n    }\n  })();\n</script>",
        "color_scheme": "",
        "padding_top": 0,
        "padding_bottom": 0
      }
    }
