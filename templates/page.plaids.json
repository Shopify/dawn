    "image_with_text_KCymWy": {
      "type": "image-with-text",
      "blocks": {
        "heading_H3a7Fq": {
          "type": "heading",
          "settings": {
            "heading": "Lan Joins",
            "heading_size": "h2"
          }
        },
        "text_pLyrNC": {
          "type": "text",
          "settings": {
            "text": "<p>Lan joined that summer after he graduated from college while he was interviewing for \"real jobs.\" He was about to leave for Chicago when Jimmy called our dad and begged him to let him stay. Thank God he did and we started selling to retailers.</p>",
            "text_style": "body"
          }
        }
