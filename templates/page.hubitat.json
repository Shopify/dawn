        "text-1": {
          "type": "text",
          "settings": {
            "text": "<ul><li>Faster response than cloud apps<\/li><li>Doesn't rely on an internet connection<\/li><li>No required monthly fees<\/li><li>Plug-and-play setup<\/li><li>Works with <a href=\"https:\/\/apps.apple.com\/us\/app\/hubitat\/id1367244704\">iOS<\/a> & <a href=\"https:\/\/play.google.com\/store\/apps\/details?id=com.hubitat.app&hl=en_US&gl=US\">Android<\/a>\n<a href=\"https:\/\/hubitat.com\/products\" target=\"_blank\">Hub<\/a> required<\/li><li>Setup for more advanced users<\/li><li>Compatible with <a href=\"https:\/\/docs2.hubitat.com\/en\/devices\/list-of-compatible-devices\" target=\"_blank\">many devices<\/a><\/li>",
            "text_style": "body"
          }
        }
      },
