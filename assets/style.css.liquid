    background-color: #F5AC51;
    font-size: 24px;
    font-weight: 600;
    color: #F6F1E0;
    line-height: 1;
    padding: 10px 6px 7px 6px;
    border-radius: 15px;
    min-width: 180px;
    display: inline-flex;
    text-decoration: none;
    transition: opacity .2s;
    &:hover {
        opacity: .6;
    }
}

.blog-list {
    padding-bottom: 100px;
    .page-width {
      max-width: 1140px;
      padding-bottom: 100px;
    }
    .blog-list-items {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        gap: 18px 30px;
        @media only screen and (max-width: 768px) {
            grid-template-columns: repeat(2, 1fr);
            gap: 30px 10px;
        }
    }
    .blog-list-item {
        color: currentColor;
        text-decoration: none;
        transition: opacity .2s;
        &:hover {
            opacity: .6;
        }
    }
    .blog-list-item-thumbnail {
        aspect-ratio: 1/1;
        width: 100%;
        height: auto;
        object-fit: cover;
        border-radius: 10px;
    }
    .blog-list-item-body {}
    .blog-list-item-title {
        font-size: 14px;
        font-weight: bold;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 2;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    .blog-list-item-excerpt {
        font-size: 14px;
        font-weight: 200;
        display: -webkit-box;
        -webkit-box-orient: vertical;
        -webkit-line-clamp: 3;
        overflow: hidden;
        text-overflow: ellipsis;
        line-height: 1.2;
    }
}

.breadcrumb {
    padding-left: 12px;
    padding-right: 12px;
    ul {
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 2px;
        list-style: none;
        padding-left: 0;
    }
    li {
        color: currentColor;
        white-space: nowrap;
        & + li {
            &:before {
                content: '/';
                margin-right: 2px;
            }
            @media only screen and (max-width: 768px) {
                white-space: nowrap;
                overflow: hidden;
                text-overflow: ellipsis;
            }
        }
        a {
            text-decoration: none;
            color: currentColor;
            transition: opacity .2s;
            &:hover {
                opacity: .6;
            }
        }
    }
}