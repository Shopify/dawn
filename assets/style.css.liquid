/*
 	3.1 multicolumn
====================================*/
.multicolumn-card__title {
    display: none;
}

.multicolumn .title {
    letter-spacing: 0;
}

.multicolumn-card__image-wrapper .media .multicolumn-card__link-wrap {
    display: flex;
    flex-direction: column;
    justify-content: center;
    row-gap: 31px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
}

.multicolumn-card__link-wrap a {
    font-size: 20px;
    font-weight: 300;
    line-height: 1.6;
    text-decoration: none;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 5px;
    padding-top: 1px;
    width: 100%;
    max-width: 295px;
    margin-left: auto;
    margin-right: auto;
    transition: 0.5s;
}

.multicolumn-card__link-wrap a:hover {
    opacity: 0.7;
}

.multicolumn-card__link-wrap .multicolumn-card__link1 {
    background: #72B1A5;
    color: #fff;
}

.multicolumn-card__link-wrap .multicolumn-card__link2 {
    background: #f54b09;
    color: #fff;
}

@media only screen and (max-width: 768px) {
    .multicolumn-card__link-wrap {
        row-gap: 19px;
    }

    .multicolumn-card__link-wrap a {
        font-size: 20px;
    }
}

/*====== #first-box ======*/
[id^=first-box] .multicolumn-card-spacing {
    padding-top: 0;
    margin-left: 0rem;
    margin-right: 0rem;
}

[id^=first-box] .multicolumn-list {
    gap: 10px;
}

[id^=first-box] .grid--3-col-desktop .grid__item {
    width: calc(100% / 3 - 10px);
    max-width: calc(100% / 3 - 10px);
}

[id^=first-box] .multicolumn-card {
    position: relative;
}

[id^=first-box] .multicolumn-card.content-container::after {
    display: none;
}

[id^=first-box] .multicolumn-card__info {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: rgba(249, 240, 240, 0.7);
    text-align: center;
    padding: 10px;
}

[id^=first-box] .multicolumn-card__info h3 {
    color: var(--jb-color-accent2-400);
    font-size: 15px;
    font-weight: 300;
}

[id^=first-box] .multicolumn-card__info .rte p {
    min-height: 153px;
    display: flex;
    align-items: center;
}

[id^=first-box] .title {
    color: var(--jb-color-accent2-400);
}

@media only screen and (min-width: 769px) {
    [id^=first-box] .multicolumn-card {
        overflow: hidden;
    }

    [id^=first-box] .multicolumn-card:hover .multicolumn-card__info {
        transform: translateY(0);
    }

    [id^=first-box] .multicolumn-card__info {
        transform: translateY(100%);
        transition: transform .3s;
    }
}

@media only screen and (max-width: 768px) {
    [id^=first-box] .multicolumn-list {
        gap: 15px 22px;
    }

    [id^=first-box] .grid--3-col-desktop .grid__item {
        width: calc(50% - 11px);
        max-width: calc(50% - 11px);
    }

    [id^=first-box] .multicolumn-card__info {
        padding: 4px;
    }

    [id^=first-box] .multicolumn-card__info h3 {
        font-size: 10px;
        font-weight: bold;
    }

    [id^=first-box] .multicolumn-card__info .rte p {
        min-height: unset;
        font-size: 14px;
    }

    [id^=first-box] .multicolumn-list .multicolumn-card__info {
        padding-left: 6px !important;
        padding-right: 6px !important;
    }
}

