

.container {
  display: flex;
  align-items: center;
  position: relative;
  padding: 40px 40px 0;
  margin-top: 60px;
}

.step-container {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  margin-top: 20px;
}

.step-label {
  border-radius: 50px;
  padding: 5px 15px;
  font-family: 'NanumPenScript'
  , sans-serif;
  font-size: 16px;
  transform: rotate(-5deg);
  margin-right: 10px;
  position: absolute;
  top: -40px;
  left: -40px;
  font-size: 36px;
  min-width: 153px;
  text-align: center;
  color: black;
  text-transform: uppercase;
}

.title {
  font-family: 'TrimPosterWebCompressed'
  , sans-serif;
  color: black;
  font-size: 36px;
  text-transform: uppercase;
  margin: 0;
  z-index: 2;
  font-size: 100px;
  letter-spacing: 1px;
  line-height: 1;
}

.blend-container {
  max-width: 990px;
  margin: 0 auto;
  position: relative;
}

.blend-description {
  margin-left: 40px;
  margin-top: 0;
  max-width: 58ch;
  font-family: 'Raleway-Thin'
  , sans-serif;
  font-size: 18px;
  color: black;
  font-weight: 700;
}

.blend-dots {
  display: flex;
  align-items: center;
  min-height: 60px;
  margin-left: 40px;
  /* width: 100%; */
  max-width: 485px;
  overflow-x: hidden;
}

.blend-bold {
  font-family: 'Raleway-Bold'
  , sans-serif;
  font-weight: 700;
  font-size: 20px;
  margin-top: 10px;
}

.blend-span {
  font-family: "Raleway"
  , sans-serif;
  font-weight: 400;
  font-size: 18px;
  line-height: 28px;
  color: #000000;
  text-decoration: underline;
  transition: text-decoration-style 0.2s;
  text-underline-offset: 5px;
  cursor: pointer;
}

.blend-span:hover {
  transition: text-decoration-style 0.2s;
  text-decoration-style: wavy;
}

.control-button {
  background-color: #000000;
  display: flex;
  align-items: center;
  justify-content: center;
  min-width: 40px;
  width: 40px !important;
  height: 40px;
  border-radius: 50%;
  cursor: pointer;
 
  &.right-control {
    margin-left: 20px;
  } 
  &.left-control {
    margin-right: 20px;
  }
}

.control-button img {
  width: 20px;
  height: 20px;
}

.blend-container-dots {
  display: flex;
  align-items: center;
  /* max-width: 65%; */
  /* margin-left: 40px; */

}

.blend-dot {
  min-width: 24px;
  min-height: 24px;
  width: 24px;
  height: 24px;
  border-radius: 50%;
  background-color: black;
  margin-right: 10px;
  transition: all 0.3s;
  margin-right: 30px;
}

.blend-dot:hover {
  cursor: pointer;
  transition: all 0.3s;
  width: 60px;
  height: 60px;
  min-width: 60px;
  min-height: 60px;
}

.blend-dot.active {
  cursor: pointer;
  transition: all 0.3s;
  min-width: 60px;
  min-height: 60px;
  width: 60px;
  height: 60px;
}

.blend-input {
  display: flex;
  max-width: 100%;
  align-items: flex-end;
}

@media (max-width: 768px) {

  .label-layer#outer {
    top: 41% !important;
    right: 29% !important;

  }

  .blend-button button {
    width: 100%;
    max-width: 100% !important;
  }
  .blend-input {
    flex-direction: column;
    max-width: 100%;
    justify-content: flex-start;
    padding: 0 20px;
  }
  .blend-input input {
    font-size: 16px !important;
    margin-left: 40px !important;
    width: Calc(100% - 20px) !important;
  }

  .blend-description-mobile {
    max-width: unset !important;
    margin-left: 20px !important;

  }
}

.blend-input input {
  width: 100%;
  padding: 10px;
  font-size: 30px;
  font-family: NanumPenScript, sans-serif;
  font-weight: 300;
  font-style: normal;
  border: none;
  border-bottom: 2px dotted black;
  margin-top: 10px;
  margin-left: 30px;
  max-height: 62px;
  outline: none;
  box-shadow: none;
}

.blend-input input::placeholder {
  color: #cccccc;
}

.blend-button {
  margin-left: 40px;
}

.blend-button button {
  width: 100%;
  max-width: 100%;
  margin-bottom: 20px;
  padding: 20px;
  border: none;
  outline: none;
  box-shadow: none;
  cursor: pointer;
  border-radius: 12px;
  font-family: 'Raleway-Bold'
  , sans-serif;
  background-color: #199776;
  transition: all 0.3s;
  color: white;
  font-size: 24px;
}

.blend-button button:hover {
  transition: all 0.3s;
  background-color: #237f5e;
}

.blend-button button:disabled {
  background: #cccccc;
  color: #666666;
}

/* flavour css */
.flavour-container {
  display: flex;
  align-items: center;
  margin-left: 40px;
  margin-top: 20px;
  position: relative;
  width: 300px;

  min-height: 300px;
  position: absolute;
  transform: translateX(300px);
  opacity: 0;
  animation: changePosition 0.4s forwards;
}

.flavours {
  min-height: 305px;
}

.flavour-image-container img {
  width: 300px;
  height: 300px;
  border-radius: 50%;
  background-size: 100%;
  background-position: center;
  animation: backgroundZoom 7s ease forwards;

}

.flavour-image-container {
  display: flex;
  align-items: center;
  position: relative;

}

.flavour-orb {
  position: absolute;
  width: 180px;
  height: 180px;
  border-radius: 50%;
  z-index: 5;
  bottom: -66px;
  left: 175px;
  opacity: 0.95;
  transition: all 1.4s;
}

.flavour-image-container img {
  overflow: hidden;

}

.flavour-detials {
  min-width: 354px;
  position: relative;
  background: #fafafa;
  z-index: 20;
  padding: 18px 100px 18px 20px;
  border-radius: 16px;

  position: absolute;
  left: 87%;
}

.flavour-name {
  font-family: "Raleway"
  , sans-serif;
  font-size: 24px;
  margin: 6px;
  font-weight: 700;
  color: black;
  line-height: 28px;

}

.flavour-controls {
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: max-content;
  position: absolute;
  bottom: 0px;
  left: 70%;
  background: #FAFAFA;
  padding: 8px;
  border-radius: 8px;
  @media screen and (min-width: 600px) {
    background: #fafafa;
    padding: 4px;
    border-radius: 12px;
    left: 82%;
    bottom: 0;
  }
  @media screen and (min-width: 1024px) {
    background: #fafafa;
    padding: 4px;
    border-radius: 12px;
    left: 82%;
    bottom: -50px;
  }
}


.flavour-control-value {
  display: block;
  font-family: "Raleway"
  , sans-serif;
  font-size: 20px;
  margin: 12px 0;
  font-weight: 700;
  color: black;
  line-height: 28px;
}

.flavour-control {
  width: 80px;
  height: 80px;
  background: #cccccc;
  font-size: 44px;
  color: #666666;
  font-weight: 700;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 12px;
  cursor: pointer;
}

.flavour-control.valid {
  background: #000000;
  color: white;
}

.flavour-description {
  font-family: "Raleway"
  , sans-serif;
  font-weight: 400;
  font-size: 14px;
  line-height: 18px;
  margin: 6px;
}


