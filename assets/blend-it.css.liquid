@keyframes backgroundZoom {
  0% {
    background-size: 120%;
  }
  100% {
    background-size: 100%;
  }
}

@keyframes bottleChange {
  0% {
    height: 100%;
  }
  50% {
    height: 10%;
  }
  100% {
    height: 100%;
  }
}

@keyframes changePosition {
  0% {
    position: absolute;
    transform: translate(300px);
    opacity: 0;
  }

  99% {
    position: absolute;
  }
  100% {
    position: relative;
    transform: translate(0px);
    opacity: 1;
  }
}

